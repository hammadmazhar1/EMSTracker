%h1 Home Page


-if !is_mfr?
	=form_tag get_help_path do
		=hidden_field_tag 'gps_latitude'
		=hidden_field_tag 'gps_longitude'
		=submit_tag 'insert help image'

	=link_to "Edit Details", edit_user_path(current_user.id)
	=link_to "Log Out", logout_path, :method => :delete
	

-else
	=link_to "Enter new case", new_case_path
	=link_to "Edit details", edit_user_path(current_user.id)
	=link_to "Log Out", logout_path, :method => :delete
